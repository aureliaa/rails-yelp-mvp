<div class='container'>
  <h3><%= @restaurant.name %></h3>
  <p><%= @restaurant.address %></p>
  <p><%= "Tel: #{@restaurant.phone_number}"%></p>
  <p><%= @restaurant.category %></p>
  <h4>Reviews</h4>
    <%= @restaurant.reviews.each do |review| %>
     <%= "#{review.rating}/5 - #{review.content}" %>
    <% end %>
</div>
